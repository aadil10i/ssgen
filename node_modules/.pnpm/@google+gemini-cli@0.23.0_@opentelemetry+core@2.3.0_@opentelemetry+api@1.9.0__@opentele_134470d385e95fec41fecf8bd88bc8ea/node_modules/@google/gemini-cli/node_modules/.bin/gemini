#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules/@google/gemini-cli/dist/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules/@google/gemini-cli/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules/@google/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules/@google/gemini-cli/dist/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules/@google/gemini-cli/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules/@google/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/@google+gemini-cli@0.23.0_@opentelemetry+core@2.3.0_@opentelemetry+api@1.9.0__@opentele_134470d385e95fec41fecf8bd88bc8ea/node_modules:/home/aadil/workspace/ssgen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
